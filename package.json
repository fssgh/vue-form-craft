    "build:deploy": "pnpm build && node ./deploy.js"
  },
  "dependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.2",
    "codemirror": "^5.65.16",
    "codemirror-editor-vue3": "^2.4.1",
    "element-plus": "^2.4.4",
    "json-editor-vue3": "^1.0.9",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "md-editor-v3": "^4.10.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vuedraggable-es": "^4.1.1"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.54.3",
    "@rushstack/eslint-patch": "^1.3.3",
