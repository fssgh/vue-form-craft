{
  "name": "vue-form-craft",
  "version": "3.0.0",
  "private": false,
  "type": "commonjs",
  "types": "./dist/types/src/release/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build-type": "run-p type-check \"build-only {@}\" --",
