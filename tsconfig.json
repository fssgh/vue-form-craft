{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "src/**/**/*",
    "docs/.vitepress/**/*",
    "docs/**/*.vue"
  ],
  "exclude": [],
  "compilerOptions": {
    "types": ["vite/client", "global", "element-plus/global"],
    // "composite": true,
    // "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "vue-form-craft": ["./src/release/index.ts"]
    },
    "allowJs": true
  }
}
